,@DebugPrefix       nvarchar(max)
,@DebugMessage      nvarchar(max)
,@index             int
,@spLength          int
,@DebugLevel        tinyint
,@ErrorLevel        int
,@InfoLevel         int
  
SET @ErrorLevel   = 3 
SET @InfoLevel    = 1
SET @DataSetMessage = 'Process Monitoring: Process Health State DataSet'
